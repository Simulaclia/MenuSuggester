      <ComboBox fx:id="cookingcombo" layoutX="1050.0" layoutY="375.0" />
      <Label alignment="CENTER" layoutX="150.0" layoutY="500.0" text="カロリー:">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <ComboBox fx:id="kcal" layoutX="350.0" layoutY="500.0" />
