<AnchorPane fx:id="randomsnallow" prefHeight="900.0" prefWidth="1600.0" stylesheets="@RSPNarrow.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="top.randomsuggest.RandomNarrowdownController">
   <children>
      <Label fx:id="title1" alignment="CENTER" layoutY="50.0" prefWidth="1600.0" text="ランダム献立機能">
