      </Label>
      <Label fx:id="title2" alignment="CENTER" layoutY="125.0" prefWidth="1600.0" text="絞り込み条件設定">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <Button fx:id="returnbuton" layoutX="69.0" layoutY="25.0" mnemonicParsing="false" onAction="#ReturnRandomSuggestPage_OnClick" prefHeight="112.0" prefWidth="352.0" text="前の画面へ">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="600.0" layoutY="250.0" text="ジャンル:">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <ComboBox fx:id="genrecombo" layoutX="850.0" layoutY="225.0" style="-fx-textsize: 48;" />
