      </Label>
      <ComboBox fx:id="time" layoutX="1185.0" layoutY="500.0" />
      <Button fx:id="okbutton" layoutX="1050.0" layoutY="650.0" mnemonicParsing="false" onAction="#okbuttonNarrowdown_OnClick" prefHeight="112.0" prefWidth="273.0" text="決定">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
      <Button fx:id="reset" layoutX="350.0" layoutY="650.0" mnemonicParsing="false" onAction="#ResetNarrowdownPage_OnClick" prefHeight="112.0" prefWidth="286.0" text="リセット">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
      <Label fx:id="noResultLabel" alignment="CENTER" layoutY="775.0" prefWidth="1600.0">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <Label fx:id="noResultLabel2" alignment="CENTER" layoutY="825.0" prefWidth="1600.0">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
